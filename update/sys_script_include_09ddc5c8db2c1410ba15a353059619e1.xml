<?xml version="1.0" encoding="UTF-8"?><record_update table="sys_script_include">
    <sys_script_include action="INSERT_OR_UPDATE">
        <access>package_private</access>
        <active>true</active>
        <api_name>x_snc_datasilos.GetParentRecord</api_name>
        <caller_access/>
        <client_callable>false</client_callable>
        <description/>
        <name>GetParentRecord</name>
        <script><![CDATA[function GetParentRecord(record)
{
	if (record == null)
	{
		return false;
	}
	
	var strTable = record.getRecordClassName(); 
	
	if (strTable == 'incident_task')
	{
		var strParentIncidentSysId = record.incident;
		var grParentIncident = new GlideRecord('incident');
		if (grParentIncident.get(strParentIncidentSysId ))
		{
			return grParentIncident;
		}
		return false;
	}

	return false;
}
]]></script>
